{
  "name": "auth-service",
  "version": "1.0.0",
  "main": "dist/src/main.js",
  "types": "dist/src/main.d.ts",
  "description": "Microserviço de Autenticação",
  "scripts": {
    "build": "nest build",
    "build:lib": "cd libs/auth-lib && npm run build",
    "build:all": "npm run build:lib && nest build",
    "publish:lib": "cd libs/auth-lib && npm publish --access public",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/src/main",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:coverage": "jest --coverage --coverageReporters=lcov --maxWorkers=2",
    "test:bdd": "jest --config jest-bdd.config.js",
    "test:bdd:watch": "jest --config jest-bdd.config.js --watch",
    "test:bdd:coverage": "jest --config jest-bdd.config.js --coverage",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:studio": "prisma studio",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down --rmi all -v --remove-orphans",
    "sonar": "sonar-scanner"
  },
  "workspaces": [
    "libs/auth-lib"
  ],
  "dependencies": {
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.0.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/jwt": "^10.0.0",
    "@nestjs/mapped-types": "^2.1.0",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/swagger": "^7.0.0",
    "@prisma/client": "^5.0.0",
    "@types/uuid": "^10.0.0",
    "bcrypt": "^5.1.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "jsonwebtoken": "^9.0.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.0",
    "sonarqube-scanner": "^3.0.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.0",
    "@types/jsonwebtoken": "^9.0.0",
    "@types/node": "^20.0.0",
    "@types/uuid": "^10.0.0",
    "jest": "^29.5.0",
    "jest-cucumber": "^3.0.2",
    "jest-sonar-reporter": "^2.0.0",
    "prisma": "^5.0.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.0.0"
  },
  "prisma": {
    "schema": "prisma/schema.prisma"
  },
  "exports": {
    ".": {
      "import": "./dist/src/main.js",
      "require": "./dist/src/main.js",
      "types": "./dist/src/main.d.ts"
    },
    "./lib": {
      "import": "./dist/lib-index.js",
      "require": "./dist/lib-index.js",
      "types": "./dist/lib-index.d.ts"
    }
  }
}
